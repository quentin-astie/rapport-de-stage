/*==============================================================*/
/*______________________ Texte - Global ________________________*/
/*==============================================================*/

p {
  vertical-align: baseline;
  font-family: 'Ume double stroke master condensed';
}

p, .informations, .intro {
  margin-bottom: 6pt;
}

.subhead {
  page-break-inside: avoid;
  page-break-after: avoid;
  page-break-before: avoid;

  margin-top: -16pt;
  padding: 6pt 0pt 12pt 0pt;
}

/*
---------------- début des titres

*/

h3, h4 {
  page-break-inside: avoid;
  page-break-after: avoid;
  page-break-before: avoid;

  margin: 0;
  padding: 6pt 0pt;
}

//titres
h1 {
  font-family: 'Ume double stroke 12';
  font-size: 40pt;
  line-height: calc(~"4 x @line-height");
  text-align: center;
  letter-spacing: -0.08em;
}

// variations titres
.stroke05 {
  font-family: 'Ume double stroke 05';
}

.stroke07 {
  font-family: 'Ume double stroke 07';
}

.stroke10 {
  font-family: 'Ume double stroke 10';
  letter-spacing: -0.08em;
}

.stroke12 {
  font-family: 'Ume double stroke 12';
  letter-spacing: -0.08em;
}

// auteurs
h2 {
  font-family: 'Ume double stroke 07';
  font-size: 24pt;
  line-height: calc(~"1.5 x @line-height");
  padding: 10pt 0pt 0pt 0pt;
}

// sous-titres
h3 {
  font-family: 'Ume double stroke info pratique';
  font-size: 13pt;
  line-height: calc(~"1.5 x @line-height");
}


h4 {
  font-family: 'Ume double stroke info pratique';
  font-size: 9pt;
  line-height: calc(~"-1.5 x @line-height");
}

//cas particuliers
.maitre{
  font-family: 'ume double stroke 10';
  letter-spacing: 0em;
}

.contemporain{
  font-family: 'ume double stroke 10';
  letter-spacing: -0.08em;
}

.orshould{
  font-family: 'ume double stroke 07';
  letter-spacing: 0em;
  padding-right: 5pt;
}

.coulisse {
  font-family: 'ume double stroke 12';
  letter-spacing: -0.08em;
}

.fortunes {
  font-family: 'ume double stroke 10';
  letter-spacing: -0.08em;
}


/*
---------------- fin des titres
*/


hr {
  visibility: hidden;
}

a{
  text-decoration: none;
  // text-decoration-color: #ff546d;
}

ul {
  list-style: none;
  font-family: 'Ume double stroke master condensed';
  padding-top: @line-height;
}

li {
  list-style: none;
}

img {
  max-width: 75pt /*100px*/ ;
  max-height: 45pt /*60px*/ ;
  background-color: white;
  // -webkit-filter: grayscale(1);
  // filter: grayscale(1);
}

.united {
  height: 25px;
  width: 95px;
  float: left;
  padding-right: 6px;
  padding-bottom: 14px;
  margin-top: -8px;
}

.logo {
  max-height: 22.5pt /*30px*/;
  outline: none;
  margin-top: 0;
  margin-left: calc(~"2 x @line-height");
  margin-bottom: @line-height;
}

// autres images
.autre {
  padding: 0;
  margin: 0;
  outline: none;
  width: auto;
}



